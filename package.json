{
  "scripts": {
    "lint": "standard --fix preprocessor.js ./test/**/*.js",
    "prepublish": "NODE_ENV=production npm run-script tsc",
    "ci": "tsc && NODE_ENV=development jest --coverage @*",
    "ci-codecov": "codecov",
    "tsc": "tsc",
    "tsc-watch": "tsc --watch",
    "docs": "typedoc --ignoreCompilerErrors --mode modules --excludePrivate --excludeExternals --theme minimal --readme ./README.md --out ./docs/ ./src/",
    "play": "NODE_ENV=production node -r source-map-support/register lib/playGame.js",
    "play-only": "NODE_ENV=production node -r source-map-support/register lib/playGame.js",
    "play-debug": "LOG_LEVEL=debug NODE_ENV=development node --inspect -r source-map-support/register ./lib/playGame.js @* 2> ./play.log",
    "play-dev": "LOG_LEVEL=debug NODE_ENV=development node -r source-map-support/register ./lib/playGame.js 2> ./play.log",
    "start": "NODE_ENV=production node -r source-map-support/register ./lib/runGames.js",
    "start-dev": "LOG_LEVEL=debug NODE_ENV=development node -r source-map-support/register ./lib/runGames.js 2> ./start.log",
    "start-coverage": "LOG_LEVEL=debug NODE_ENV=development istanbul cover -x '**/node_modules/**' ./lib/runGames.js 2> ./start.log",
    "start-debug": "LOG_LEVEL=debug NODE_ENV=development node --inspect-brk lib/runGames.js 2> ./start.log",
    "start-debug-cli": "LOG_LEVEL=debug NODE_ENV=development node inspect lib/runGames.js 2> ./start.log",
    "poststart": "npm run-script coverage && echo 'Generated coverage. Type open ./coverage/lcov-html/index.html to see it'",
    "coverage": "istanbul report --root ./coverage/ lcov",
    "pretest": "tsc",
    "test": "NODE_ENV=development jest --coverage --updateSnapshot --verbose",
    "posttest": "npm run-script test-cli && npm run-script test-webpackable",
    "test-debug": "NODE_ENV=development node --inspect-brk -r source-map-support/register $(npm bin)/jest -i --updateSnapshot $@",
    "test-cli": "./bin/puzzlescript.js --no-ui --game 'Beam Islands' --level 1 --size small --resume",
    "test-webpackable": "webpack-command ./lib/index.js --mode production --devtool source-map --output ./test/webpack-output.js",
    "watch": "jest --updateSnapshot --watch",
    "debug": "NODE_ENV=development node --inspect-brk lib/runGames.js"
  },
  "bin": {
    "puzzlescript": "./bin/puzzlescript.js",
    "puzzlescript-demo": "./bin/puzzlescript-demo.js"
  },
  "dependencies": {
    "ansi-escapes": "^3.1.0",
    "ansi-styles": "^3.2.1",
    "bitset": "^5.0.3",
    "chalk": "^2.4.1",
    "commander": "^2.16.0",
    "eventemitter2": "^5.0.1",
    "firstline": "^2.0.2",
    "glob": "^7.1.2",
    "inquirer": "6.0.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jest": "^23.3.0",
    "lodash": "^4.17.10",
    "ohm-js": "^0.14.0",
    "pify": "^3.0.0",
    "supports-color": "^5.4.0",
    "update-notifier": "^2.5.0",
    "web-audio-api": "^0.2.2"
  },
  "devDependencies": {
    "@types/ansi-escapes": "^3.0.0",
    "@types/ansi-styles": "^3.2.0",
    "@types/glob": "^5.0.35",
    "@types/inquirer": "0.0.42",
    "@types/lodash": "^4.14.108",
    "@types/pify": "^3.0.2",
    "@types/supports-color": "^5.3.0",
    "codecov": "^3.0.2",
    "concurrently": "^3.6.0",
    "eslint-plugin-typescript": "^0.12.0",
    "http-server": "^0.11.1",
    "istanbul": "^0.4.5",
    "jest": "^23.3.0",
    "jest-watch-typeahead": "^0.2.0",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.1.2",
    "puppeteer": "^1.6.0",
    "source-map-support": "^0.5.6",
    "standard": "^11.0.1",
    "tslint": "^5.10.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.3",
    "typescript-eslint-parser": "17.0.0",
    "webpack": "^4.16.2",
    "webpack-command": "^0.4.1"
  },
  "optionalDependencies": {
    "speaker": "github:philschatz/node-speaker#cac7c5b7c434b52619bfbbd9530ab0871156e815"
  },
  "jest": {
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      ".ts": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.+(ts|js)"
    ]
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "typescript"
    ]
  },
  "name": "puzzlescript-cli",
  "description": "Play PuzzleScript games in your terminal!",
  "version": "1.5.0",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philschatz/puzzlescript-cli.git"
  },
  "keywords": [
    "puzzlescript",
    "game",
    "console",
    "terminal"
  ],
  "author": "Philip Schatz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/philschatz/puzzlescript-cli/issues"
  },
  "homepage": "https://github.com/philschatz/puzzlescript-cli"
}
